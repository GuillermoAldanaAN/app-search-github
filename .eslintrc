{
    "extends": [
        "react-app",
        "standard",
        "plugin:react/recommended",
        "prettier",
        "prettier/react",
        "eslint-config-prettier"
    ],
    "plugins": [
        "jsx-a11y",
        "prettier"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ]
    }
}